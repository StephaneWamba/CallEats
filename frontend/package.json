    "assess:all": "npm run assess:refactoring && npm run assess:dependencies"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
